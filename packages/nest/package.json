    "@nestjs/jwt": "^10.1.1",
    "@nestjs/platform-fastify": "^10.2.5",
    "@nestjs/schedule": "^3.0.3",
    "@ngneat/falso": "^6.3.2",
    "@prisma/client": "^5.6.0",
    "fastify": "^4.24.0",
    "google-auth-library": "^9.0.0",
    "graphql": "^16.8.1",
    "graphql-scalars": "^1.22.4",
    "graphql-subscriptions": "^2.0.0",
    "ics": "^3.5.0",
